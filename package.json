{
  "name": "codemo",
  "private": true,
  "description": "Colder's Demo Codes",
  "keywords": [
    "demo"
  ],
  "author": {
    "name": "colder"
  },
  "workspaces": [
    "*"
  ],
  "prettier": {
    "importOrder": [
      "^react(/.+)*$",
      "^react-dom(/.+)*$",
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderSortSpecifiers": true,
    "printWidth": 100,
    "singleQuote": true
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@tsconfig/recommended": "^1.0.1",
    "http-server": "^13.0.2",
    "prettier": "^2.5.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "prettier-plugin-packagejson": "^2.2.15",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "16"
  }
}
